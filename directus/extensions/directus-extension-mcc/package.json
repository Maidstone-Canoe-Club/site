{
	"name": "directus-extension-mcc",
	"version": "1.0.0",
	"type": "module",
	"dependencies": {
		"stripe": "^13.5.0"
	},
	"directus:extension": {
		"host": "^10.1.9",
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "endpoint",
				"name": "webhooks",
				"source": "src/webhooks/index.ts"
			},
			{
				"type": "hook",
				"name": "mail-forward-hooks",
				"source": "src/mail-forward-hooks/index.ts"
			},
			{
				"type": "endpoint",
				"name": "invites",
				"source": "src/invites/index.ts"
			},
			{
				"type": "endpoint",
				"name": "confirm-email",
				"source": "src/confirm-email/index.ts"
			},
			{
				"type": "hook",
				"name": "user-created-hook",
				"source": "src/user-created-hook/index.ts"
			},
			{
				"type": "endpoint",
				"name": "registration",
				"source": "src/registration/index.ts"
			},
			{
				"type": "endpoint",
				"name": "content",
				"source": "src/content/index.ts"
			},
			{
				"type": "endpoint",
				"name": "juniors",
				"source": "src/juniors/index.ts"
			},
			{
				"type": "endpoint",
				"name": "checkout",
				"source": "src/checkout/index.ts"
			},
			{
				"type": "endpoint",
				"name": "payments",
				"source": "src/payments/index.ts"
			}
		]
	},
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-bundle"
	],
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"add": "directus-extension add",
		"lint": "eslint --fix --ext .ts"
	},
	"devDependencies": {
		"@directus/errors": "^0.0.2",
		"@directus/extensions-sdk": "10.1.9",
		"@types/node": "^20.5.8",
		"@typescript-eslint/eslint-plugin": "^6.5.0",
		"@typescript-eslint/parser": "^6.5.0",
		"eslint": "^8.48.0",
		"express": "^4.18.2",
		"nanoid": "^4.0.2",
		"nanoid-dictionary": "^4.3.0",
		"ofetch": "^1.3.3",
		"typescript": "^5.2.2",
		"stripe": "^13.5.0"
	}
}
